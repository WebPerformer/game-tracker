{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "autostart:allow-enable",
    "dialog:allow-message",
    "store:allow-load",
    "store:allow-get-store",
    "store:allow-set",
    "store:allow-get",
    "store:allow-save"
  ]
}
